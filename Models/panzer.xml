<PropertyList>
 <path>panzer.ac</path>
        
<offsets>
<heading-deg>90.0</heading-deg>
<z-m>0.3</z-m>
<x-m>2.45</x-m>
</offsets>

        <!-- rotate turret -->
        <animation>
                <object-name>turret01</object-name>
                <object-name>barrel</object-name>
                <type>spin</type>
                <property>controls/flight/aileron</property>
                <factor>-1</factor>
                <center>
                        <x-m>0</x-m>
                        <y-m>0</y-m>
                        <z-m>0.1</z-m>
                </center>
                <axis>
                        <x>0</x>
                        <y>0</y>
                        <z>1</z>
                </axis>
        </animation>
 
        <!-- shift barrel-->
        <animation>
                <object-name>barrel</object-name>
                <type>rotate</type>
                <property>controls/flight/elevator</property>
                <factor>-6.5</factor>
                <offset-deg>-5</offset-deg>
                <min>-3</min>
                <max>35</max>
                <center>
                        <x-m>-0.117</x-m>
                        <y-m>-0.499</y-m>
                        <z-m>1.910</z-m>
                </center>
                <axis>
                        <x>1</x>
                        <y>0</y>
                        <z>0</z>
                </axis>
        </animation>

<!-- Start Extra Details -->

